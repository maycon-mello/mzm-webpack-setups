    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.0.5",
    "node-sass": "^4.8.3",
    "nyc": "^11.6.0",
    "optimist": "^0.6.1",
    "react-relay": "^1.5.0",
    "react-test-renderer": "^16.3.0",
    "relay-compiler": "^1.5.0",
    "sass-loader": "^6.0.7",
    "sinon": "^4.4.9",
    "sinon-chai": "^3.0.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack --mode development",
    "build": "NODE_ENV=production webpack --mode production",
    "start": "NODE_ENV=development webpack-dev-server --mode development --open --history-api-fallback",
    "test": "NODE_ENV=test babel-node test/runTest",
    "test:watch": "yarn test --watch --reporter nyan",
    "coverage": "nyc -n src -c false --ignore-class-method render --check-coverage --branches 100 --statements 100 --lines 100 --functions 100 -e .js npm run test",
    "coverage:watch": "watch 'yarn coverage' src",
    "eslint": "eslint --ext .js src test",
    "eslint:watch": "watch 'yarn eslint' src test",
    "relay": "relay-compiler --src ./src --schema path/schema.graphql --extensions js"
  },
  "dependencies": {
    "enzyme": "^3.3.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-loadable": "^5.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
